# Altitude Recall Monitor - Environment Configuration
# Copy this file to .env and fill in your values

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=true

# Database (SQLite by default)
DATABASE_URL=sqlite+aiosqlite:///./altitude.db

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# CPSC API (public, no key needed)
CPSC_API_BASE_URL=https://www.saferproducts.gov/RestWebServices

# Visual Search API Keys (optional)
GOOGLE_VISION_API_KEY=
TINEYE_API_KEY=

# Agent Defaults
DEFAULT_SEARCH_FREQUENCY=60
DEFAULT_SEARCH_DEPTH=3
DEFAULT_MATCH_SENSITIVITY=0.7



